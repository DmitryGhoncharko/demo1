<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="org.example.demo1.ActionListController" prefHeight="400.0" prefWidth="600.0">
    <TableView fx:id="actionTableView" prefHeight="360.0" prefWidth="560.0">
        <columns>
            <TableColumn fx:id="idColumn" text="ID" />
            <TableColumn fx:id="infoColumn" text="Информация" />
            <TableColumn fx:id="imageColumn" text="Изображение" />
            <TableColumn fx:id="deleteColumn" text="Удалить" />
        </columns>
    </TableView>
    <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="560.0" text="Назад" onAction="#handleBackButtonAction"/>
</VBox>
