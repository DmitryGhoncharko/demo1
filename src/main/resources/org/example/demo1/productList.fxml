<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.demo1.ProductListController"
            prefHeight="400.0" prefWidth="600.0">
    <TableView fx:id="productTableView" layoutX="20" layoutY="20" prefHeight="360.0" prefWidth="560.0">
        <columns>
            <TableColumn fx:id="idColumn" text="ID" />
            <TableColumn fx:id="nameColumn" text="Название" />
            <TableColumn fx:id="dateColumn" text="Дата" />
            <TableColumn fx:id="imageColumn" text="Изображение" />
            <TableColumn fx:id="deleteColumn" text="Удалить" />
        </columns>
    </TableView>
</AnchorPane>
